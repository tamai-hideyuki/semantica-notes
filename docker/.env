# ── プロジェクト／ポート設定 ──
COMPOSE_PROJECT_NAME=semantica-notes
NGINX_PORT=8000
FRONTEND_PORT=11000
BACKEND_PORT=12000

# ── フロントエンド API 基底 URL ──
NEXT_PUBLIC_API_BASE_URL=http://localhost:${NGINX_PORT}/

# ── ベクトル検索アプリ設定 ──
VEC_EMBEDDING_DIM=768
VEC_ELASTICSEARCH_HOSTS=http://elasticsearch:9200
VEC_ELASTICSEARCH_INDEX=memos
VEC_HYBRID_SEMANTIC_WEIGHT=0.7
VEC_HYBRID_ELASTIC_WEIGHT=0.3
